{
    "name": "flatorb/moldssp",
    "description": "A package library to use laravel pagination with datatables and mongodb.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel", "mongodb", "datatables", "ssp", "pagination", "server side processing"
    ],
    "authors": [
        {
            "name": "Anushan W",
            "email": "packages@flatorb.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Flatorb\\Moldssp\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flatorb\\Moldssp\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flatorb\\Moldssp\\MoldsspServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
